# Environment Configuration for Dev API Vault
# This file contains example values for environment variables

# API Security Configuration
RAPIDAPI_PROXY_SECRET=example_secret_key_change_this_in_production

# FastAPI Configuration
FASTAPI_ENV=development
DEBUG=True

# CORS Configuration (if needed)
ALLOWED_ORIGINS=["http://localhost:3000", "https://your-frontend-domain.com"]

# Rate Limiting Configuration
RATE_LIMIT_REQUESTS_PER_MINUTE=60

# Logging Configuration
LOG_LEVEL=INFO

# External API Timeouts (in seconds)
REQUEST_TIMEOUT=10